# .env.example
# Copy to .env and fill with actual values

# MongoDB
MONGO_INITDB_ROOT_USERNAME=admin_username
MONGO_INITDB_ROOT_PASSWORD=secure_password_here

# AWS Credentials
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Airbyte Database (if using local Airbyte)
AIRBYTE_DB_USERNAME=airbyte_user
AIRBYTE_DB_PASSWORD=supersecretpassword
AIRBYTE_DB_URL=jdbc:postgresql://airbyte-db:5432/airbyte
AIRBYTE_SERVER_PORT=8000
AIRBYTE_WEBAPP_PORT=8001

# Mongo Express (for monitoring)
MONGO_EXPRESS_PASSWORD=monitoring_password